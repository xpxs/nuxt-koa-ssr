{
  "name": "koa-nuxt-ssr",
  "version": "1.1.0",
  "description": "nuxt后端渲染",
  "author": "junner8909 <908900776@qq.com>",
  "private": true,
  "scripts": {
    "fix-memory-limit": "cross-env LIMIT=4096 increase-memory-limit",
    "dev": "cross-env __ENV='development' && backpack dev",
    "alpha:build": "cross-env __ENV=alpha && nuxt build && backpack build",
    "alpha:start": "cross-env __ENV=alpha node build/main.js",
    "alpha:pm2": "cross-env __ENV=alpha pm2 start build/main.js --name='koa-nuxt-ssr-alpha'",
    "uat:build": "cross-env __ENV=uat && nuxt build && backpack build",
    "uat:start": "cross-env __ENV=uat node build/main.js",
    "uat:pm2": "cross-env __ENV=production pm2 start build/main.js --name='koa-nuxt-ssr-prod'",
    "prod:build": "cross-env __ENV=production && nuxt build && backpack build",
    "prod:start": "cross-env __ENV=production node build/main.js",
    "prod:pm2": "cross-env __ENV=production pm2 start build/main.js --name='koa-nuxt-ssr-prod'",
    "precommit": "npm run lint",
    "generate": "nuxt generate",
    "analyze": "nuxt bulid --analyze",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "axios": "0.18.0",
    "bcryptjs": "2.4.3",
    "cross-env": "5.0.1",
    "es6-promise": "4.2.5",
    "fs": "0.0.1-security",
    "increase-memory-limit": "1.0.6",
    "iview": "3.1.3",
    "koa": "2.4.1",
    "koa-bodyparser": "4.2.1",
    "koa-convert": "1.2.0",
    "koa-cors": "0.0.16",
    "koa-jwt": "3.5.1",
    "koa-log4": "2.3.0",
    "koa-router": "7.4.0",
    "lodash": "4.17.11",
    "mockjs": "1.0.1-beta3",
    "moment": "2.22.2",
    "node-sass": "4.10.0",
    "nuxt": "2.3.2",
    "path": "0.12.7",
    "sass-loader": "7.1.0",
    "sequelize": "4.39.1",
    "source-map-support": "0.4.15",
    "vue-loader": "15.4.2",
    "vue-lodash": "2.0.0",
    "vuex": "3.0.1"
  },
  "devDependencies": {
    "@nuxtjs/axios": "5.3.1",
    "@nuxtjs/proxy": "1.2.4",
    "babel-core": "6.26.0",
    "babel-eslint": "6.1.1",
    "babel-loader": "8.0.4",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-0": "6.24.1",
    "backpack-core": "0.7.0",
    "eslint": "5.7.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-config-standard": "12.0.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-html": "3.0.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-node": "7.0.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-promise": "4.0.0",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-vue": "4.7.1",
    "mysql2": "1.6.1",
    "nodemon": "1.11.0",
    "prettier": "1.14.3",
    "scmp": "2.0.0",
    "vue-particles": "1.0.9"
  }
}
